{
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch via NPM",
      "request": "launch",
      "runtimeArgs": [
        "run-script",
        "debug"
      ],
      "runtimeExecutable": "pnpm",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "type": "node"
    },

    {
      "name": "pnpm run start",
      "type": "chrome",
      "request": "launch",
      "preLaunchTask": "pnpm: start",
      "url": "http://localhost:4200/"
    },
  ]
}
