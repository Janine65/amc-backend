  "version": "0.2.0",
  "configurations": [
    {
      "command": "pnpm start",
      "name": "Run npm start",
      "request": "launch",
      "type": "node-terminal"
    },
  {
    "name": "Launch Chrome",
    "type": "chrome",
    "request": "launch",
    "url": "http://localhost:4200",
    "webRoot": "${workspaceFolder}"
  },
  {
    "name": "Attach to Chrome",
    "type": "chrome",
    "request": "attach",
    "port": 9222,
    "webRoot": "${workspaceFolder}"
  },
  ],
}
